!```````````````````````````````````````````````````````````````````
pure function eq_fp_f4_1d(A, B)  result(is_equal_to)

!+ Tests for element-wise equality between the values of two given
!   4-byte floating-point, 1-dimensional arrays of the same shape and size.

use univ_kind_defs_mod, only: f4

implicit none

!=== Argument declarations:

real(f4), dimension(:), intent(IN) :: A, B

!== Local declarations:

logical, dimension(size(A, 1)) ::  &
     is_equal_to  ! Procedure result

!=== Executable statements:

is_equal_to = ( abs(A-B) < spacing(max(abs(A), abs(B))) )

end function eq_fp_f4_1d


