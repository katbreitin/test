!```````````````````````````````````````````````````````````````````
pure function eq_fp_f8_6d(A, B)  result(is_equal_to)

!+ Tests for element-wise equality between the values of two given
!   8-byte floating-point, 6-dimensional arrays of the same shape and size.

use univ_kind_defs_mod, only: f8

implicit none

!=== Argument declarations:

real(f8), dimension(:,:,:,:,:,:), intent(IN) :: A, B

!== Local declarations:

logical, dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     is_equal_to  ! Procedure result

!=== Executable statements:

is_equal_to = ( abs(A-B) < spacing(max(abs(A), abs(B))) )

end function eq_fp_f8_6d


!```````````````````````````````````````````````````````````````````
pure function ne_fp_f8_6d(A, B)  result(is_notequal_to)

!+ Tests for element-wise inequality between the values of two given
!   8-byte floating-point, 6-dimensional arrays of the same shape and size.

use univ_kind_defs_mod, only: f8

implicit none

!=== Argument declarations:

real(f8), dimension(:,:,:,:,:,:), intent(IN) :: A, B

!== Local declarations:

logical, dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     is_notequal_to  ! Procedure result

!=== Executable statements:

is_notequal_to = .not.( abs(A-B) < spacing(max(abs(A), abs(B))) )

end function ne_fp_f8_6d


!```````````````````````````````````````````````````````````````````
pure function ge_fp_f8_6d(A, B)  result(is_greater_than_or_equal_to)

!+ Tests element-wise whether the values of the first of two given
!   8-byte floating-point, 6-dimensional arrays of the same shape and size
!   are greater than or equal to the values of the other array.

use univ_kind_defs_mod, only: f8

implicit none

!=== Argument declarations:

real(f8), dimension(:,:,:,:,:,:), intent(IN) :: A, B

!== Local declarations:

real(f8), dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     sp

logical, dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     is_greater_than_or_equal_to  ! Procedure result

!=== Executable statements:

sp = spacing(max(abs(A), abs(B)))
is_greater_than_or_equal_to = ( (A-B >= sp) .or. (abs(A-B) < sp) )

end function ge_fp_f8_6d


!```````````````````````````````````````````````````````````````````
pure function le_fp_f8_6d(A, B)  result(is_less_than_or_equal_to)

!+ Tests element-wise whether the values of the first of two given
!   8-byte floating-point, 6-dimensional arrays of the same shape and size
!   are less than or equal to the values of the other array.

use univ_kind_defs_mod, only: f8

implicit none

!=== Argument declarations:

real(f8), dimension(:,:,:,:,:,:), intent(IN) :: A, B

!== Local declarations:

real(f8), dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     sp

logical, dimension(size(A, 1), size(A, 2), size(A, 3), size(A, 4), size(a, 5), size(A, 6)) ::  &
     is_less_than_or_equal_to  ! Procedure result

!=== Executable statements:

sp = spacing(max(abs(A), abs(B)))
is_less_than_or_equal_to = ( (B-A >= sp) .or. (abs(A-B) < sp) )

end function le_fp_f8_6d


